<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <context-param>
        <param-name>javax.faces.PROJECT_STAGE</param-name>
        <param-value>Development</param-value>
    </context-param>
    <servlet>
        <servlet-name>Login</servlet-name>
        <servlet-class>org.controlador.Login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Acceso</servlet-name>
        <servlet-class>org.controlador.Acceso</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>OperacionesArbitro</servlet-name>
        <servlet-class>org.controlador.OperacionesArbitro</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>OperacionesDetalle</servlet-name>
        <servlet-class>org.controlador.OperacionesDetalle</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>OperacionesEquipo</servlet-name>
        <servlet-class>org.controlador.OperacionesEquipo</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>OperacionesJornada</servlet-name>
        <servlet-class>org.controlador.OperacionesJornada</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>OperacionesJugador</servlet-name>
        <servlet-class>org.controlador.OperacionesJugador</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>OperacionesPartido</servlet-name>
        <servlet-class>org.controlador.OperacionesPartido</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>OperacionesUsuarios</servlet-name>
        <servlet-class>org.controlador.OperacionesUsuarios</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>OperacionesEntrenador</servlet-name>
        <servlet-class>org.controlador.OperacionesEntrenador</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>OperacionesIncidencias</servlet-name>
        <servlet-class>org.controlador.OperacionesIncidencias</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Faces Servlet</servlet-name>
        <servlet-class>javax.faces.webapp.FacesServlet</servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>
    <servlet>
        <servlet-name>OperacionesDetalles</servlet-name>
        <servlet-class>org.controlador.OperacionesDetalles</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>OperacionesTorneos</servlet-name>
        <servlet-class>org.controlador.OperacionesTorneos</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Reporte1</servlet-name>
        <servlet-class>org.controlador.Reporte1</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Acceso</servlet-name>
        <url-pattern>/acceso</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>OperacionesArbitro</servlet-name>
        <url-pattern>/Admin/operacionesArbitro</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>OperacionesDetalle</servlet-name>
        <url-pattern>/Admin/operacionesDetalle</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>OperacionesEquipo</servlet-name>
        <url-pattern>/Admin/operacionesEquipo</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>OperacionesJornada</servlet-name>
        <url-pattern>/Admin/operacionesJornada</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>OperacionesJugador</servlet-name>
        <url-pattern>/Admin/operacionesJugador</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>OperacionesPartido</servlet-name>
        <url-pattern>/Admin/operacionesPartido</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>OperacionesUsuarios</servlet-name>
        <url-pattern>/Admin/operacionesUsuarios</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>OperacionesEntrenador</servlet-name>
        <url-pattern>/Admin/operacionesEntrenador</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>OperacionesIncidencias</servlet-name>
        <url-pattern>/Admin/operacionesIncidencias</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Faces Servlet</servlet-name>
        <url-pattern>/faces/*</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>OperacionesDetalles</servlet-name>
        <url-pattern>/Admin/operacionesDetalles</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>OperacionesTorneos</servlet-name>
        <url-pattern>/Admin/operacionesTorneos</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Reporte1</servlet-name>
        <url-pattern>/Admin/reporte1</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>faces/index.xhtml</welcome-file>
    </welcome-file-list>
</web-app>
